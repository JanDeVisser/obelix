b = [ 2*i for i in 1 ~ 5 ]
print(b)

b = [ 2*i for i in [ 2*j for j in [ 2*k for k in 1 ~ 5 ] ] ]
print(b)

func add(sum, x)
  if sum
    return sum + x
  else
    return x
  end
end

print([ 2*i for i in 1 ~ 5 ] | add)

