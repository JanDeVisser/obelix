l = [ 2, 4, 6, 8 ]

func add(sum, x)
  if sum
    return sum + x
  else
    return x
  end
end

s = l.reduce(add)
print(s) // 20

s = l | add
print(s) // 20

s = (l | add) + 1
print(s) // 21

s = l | add <- 1
print(s) // 21

s = (l | add <- 1) + 1
print(s) // 22

/* EXPECTED
--exit-- 0
--stdout--
20
21
--
EXPECTED */
